{
  "name": "nodecg-mysteryfunhouse",
  "version": "0.1.0",
  "description": "",
  "homepage": "",
  "author": {
    "name": "Maurice",
    "email": "maurice@guegan.de",
    "url": "https://guegan.de"
  },
  "scripts": {
    "start": "node ../..",
    "dev": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "mix --production"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension"
  ],
  "keywords": [
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "name": "general",
        "title": "General",
        "width": 2,
        "file": "general.html"
      },
      {
        "name": "timer",
        "title": "Timer",
        "width": 2,
        "file": "timer.html"
      },
      {
        "name": "match-1",
        "title": "Match 1",
        "width": 4,
        "file": "match1.html",
        "headerColor": "#D32F2F"
      },
      {
        "name": "match-2",
        "title": "Match 2",
        "width": 4,
        "file": "match2.html",
        "headerColor": "#1976D2"
      },
      {
        "name": "game",
        "title": "Game",
        "width": 2,
        "file": "game.html"
      },
      {
        "name": "twitch",
        "title": "Twitch",
        "width": 2,
        "file": "twitch.html",
        "headerColor": "#9146FF"
      },
      {
        "name": "rainwave",
        "title": "Rainwave",
        "width": 2,
        "file": "rainwave.html",
        "headerColor": "#F7941E"
      },
      {
        "name": "stream-cropping",
        "title": "Stream Cropping",
        "file": "cropping.html",
        "fullbleed": true
      },
      {
        "name": "boxart-select-dialog",
        "title": "Select Boxart",
        "file": "boxart.html",
        "width": 7,
        "dialog": true
      },
      {
        "name": "load-challonge-dialog",
        "title": "Import Challonge match",
        "file": "challonge.html",
        "width": 5,
        "dialog": true
      },
      {
        "name": "player-card-preview-dialog",
        "title": "Player Card Preview",
        "file": "playercardpreview.html",
        "width": 5,
        "dialog": true
      }
    ],
    "graphics": [
      {
        "file": "mt16/2player.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "mt16/2player_tate.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "mt16/4player.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "mt16/waitscreen.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "mt16/coming_up.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "jolly/2player.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "funfest/4player.html",
        "width": 1920,
        "height": 1080
      }
    ],
    "soundCues": [
      {
        "name": "Player Dead",
        "assignable": false,
        "defaultFile": "sound/player_dead.mp3"
      }
    ],
    "assetCategories": [
      {
        "name": "boxarts",
        "title": "Game Boxarts",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      },
      {
        "name": "popovers",
        "title": "Player Popovers",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      }
    ],
    "mount": [
      {
        "directory": "dist",
        "endpoint": "dist"
      }
    ]
  },
  "repository": "github:Stabyourself/nodecg-mysteryfunhouse",
  "license": "MIT",
  "devDependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clone": "^2.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "deepmerge": "^4.2.2",
    "laravel-mix": "^6.0.39",
    "node-sass": "^6.0.1",
    "random-seed": "^0.3.0",
    "resolve-url-loader": "^4.0.0",
    "sass": "~1.32",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "three": "^0.135.0",
    "vue": "^2.6.14",
    "vue-drag-resize": "^1.5.4",
    "vue-loader": "^15.9.8",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "vue-tilt.js": "^1.1.1",
    "vuetify": "^2.6.0",
    "vuetify-loader": "^1.7.3",
    "vuetifyjs-mix-extension": "^0.0.20",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@twurple/api": "^5.0.9",
    "@twurple/auth": "^5.0.9",
    "discord.js": "^13.3.1",
    "google-spreadsheet": "^3.2.0",
    "lodash": "^4.17.21",
    "markdown-it-vue": "^1.1.6",
    "request": "^2.88.2",
    "timer-node": "^5.0.5"
  }
}
